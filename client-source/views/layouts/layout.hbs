<!doctype html>
<html lang="ru">
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
  <meta name="keywords" content="" />
  <meta name="description" content="" />
  <meta name="viewport" content="width=device-width, user-scalable=yes, initial-scale=1, maximum-scale=1">

  <title>Cookie Server :: {{ title }}</title>

  <link rel="icon" type="image/icon" href="/public/images/favicon.ico">
  <link rel="stylesheet" type="text/css" media="all" href="/public/css/fonts.css" />
  <link type="text/css" rel="stylesheet" media="all" href="/public/css/style.css" />
  <link type="text/css" rel="stylesheet" media="all" href="/public/css/menu.css" />
  <link type="text/css" rel="stylesheet" media="all" href="/public/css/selectize.css" />

  <script type="text/javascript" src="/public/js/jquery-3.4.1.min.js"></script>
  <script type="text/javascript" src="/public/js/selectize.js"></script>
  <script type="text/javascript" src="/public/js/proto-delay.js"></script>

</head>
  <body>

  {{#iff typePage '==' null}}
    {{> client/client}}
  {{/iff}}

  {{#iff typePage '==' 'control'}}
    {{> control/control}}
  {{/iff}}


  {{#if errors}}
    <div id="tip-error" style="bottom: -200px;">
      <div class="content">
        <div class="close-button"></div>
        <div class="text">{{{errorsRender errors}}}</div>
      </div>
    </div>
    <script type="text/javascript" src="/public/js/error-shower.js"></script>
  {{/if}}
  </body>
</html>

